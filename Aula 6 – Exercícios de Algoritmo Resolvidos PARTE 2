// CONVERSÃO DE  FARENAITES PARA CELSIUS

escreva("Qual a temperatura aqui?")
leia(farenaites)

 celsius <- (farenaites -32)/1.8

escreva ("No Brasil estaria: ", celsius:4:1 , "ºC")




